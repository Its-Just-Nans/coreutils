#compdef uu_uptime

autoload -U is-at-least

_uu_uptime() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" \
'-s[system up since]' \
'--since[system up since]' \
'-h[Print help]' \
'--help[Print help]' \
'-V[Print version]' \
'--version[Print version]' \
'::path -- file to search boot time from:_files' \
&& ret=0
}

(( $+functions[_uu_uptime_commands] )) ||
_uu_uptime_commands() {
    local commands; commands=()
    _describe -t commands 'uu_uptime commands' commands "$@"
}

if [ "$funcstack[1]" = "_uu_uptime" ]; then
    _uu_uptime "$@"
else
    compdef _uu_uptime uu_uptime
fi
