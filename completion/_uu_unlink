#compdef uu_unlink

autoload -U is-at-least

_uu_unlink() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" \
'-h[Print help]' \
'--help[Print help]' \
'-V[Print version]' \
'--version[Print version]' \
':FILE:_files' \
&& ret=0
}

(( $+functions[_uu_unlink_commands] )) ||
_uu_unlink_commands() {
    local commands; commands=()
    _describe -t commands 'uu_unlink commands' commands "$@"
}

if [ "$funcstack[1]" = "_uu_unlink" ]; then
    _uu_unlink "$@"
else
    compdef _uu_unlink uu_unlink
fi
