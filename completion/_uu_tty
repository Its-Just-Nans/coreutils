#compdef uu_tty

autoload -U is-at-least

_uu_tty() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" \
'-s[print nothing, only return an exit status]' \
'--silent[print nothing, only return an exit status]' \
'--quiet[print nothing, only return an exit status]' \
'-h[Print help]' \
'--help[Print help]' \
'-V[Print version]' \
'--version[Print version]' \
&& ret=0
}

(( $+functions[_uu_tty_commands] )) ||
_uu_tty_commands() {
    local commands; commands=()
    _describe -t commands 'uu_tty commands' commands "$@"
}

if [ "$funcstack[1]" = "_uu_tty" ]; then
    _uu_tty "$@"
else
    compdef _uu_tty uu_tty
fi
