#compdef uu_tee

autoload -U is-at-least

_uu_tee() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" \
'(-p)--output-error=[set write error behavior]' \
'-h[Print help]' \
'--help[Print help]' \
'-a[append to the given FILEs, do not overwrite]' \
'--append[append to the given FILEs, do not overwrite]' \
'-i[ignore interrupt signals (ignored on non-Unix platforms)]' \
'--ignore-interrupts[ignore interrupt signals (ignored on non-Unix platforms)]' \
'-p[set write error behavior (ignored on non-Unix platforms)]' \
'-V[Print version]' \
'--version[Print version]' \
'::file:_files' \
&& ret=0
}

(( $+functions[_uu_tee_commands] )) ||
_uu_tee_commands() {
    local commands; commands=()
    _describe -t commands 'uu_tee commands' commands "$@"
}

if [ "$funcstack[1]" = "_uu_tee" ]; then
    _uu_tee "$@"
else
    compdef _uu_tee uu_tee
fi
