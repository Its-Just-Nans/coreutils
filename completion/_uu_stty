#compdef uu_stty

autoload -U is-at-least

_uu_stty() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" \
'-F+[open and use the specified DEVICE instead of stdin]:DEVICE:_files' \
'--file=[open and use the specified DEVICE instead of stdin]:DEVICE:_files' \
'-a[print all current settings in human-readable form]' \
'--all[print all current settings in human-readable form]' \
'-g[print all current settings in a stty-readable form]' \
'--save[print all current settings in a stty-readable form]' \
'-h[Print help]' \
'--help[Print help]' \
'-V[Print version]' \
'--version[Print version]' \
'::settings -- settings to change:' \
&& ret=0
}

(( $+functions[_uu_stty_commands] )) ||
_uu_stty_commands() {
    local commands; commands=()
    _describe -t commands 'uu_stty commands' commands "$@"
}

if [ "$funcstack[1]" = "_uu_stty" ]; then
    _uu_stty "$@"
else
    compdef _uu_stty uu_stty
fi
