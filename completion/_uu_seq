#compdef uu_seq

autoload -U is-at-least

_uu_seq() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" \
'-s+[Separator character (defaults to \\n)]: : ' \
'--separator=[Separator character (defaults to \\n)]: : ' \
'-t+[Terminator character (defaults to \\n)]: : ' \
'--terminator=[Terminator character (defaults to \\n)]: : ' \
'-f+[use printf style floating-point FORMAT]: : ' \
'--format=[use printf style floating-point FORMAT]: : ' \
'-w[Equalize widths of all numbers by padding with zeros]' \
'--equal-width[Equalize widths of all numbers by padding with zeros]' \
'-h[Print help]' \
'--help[Print help]' \
'-V[Print version]' \
'--version[Print version]' \
'*::numbers:' \
&& ret=0
}

(( $+functions[_uu_seq_commands] )) ||
_uu_seq_commands() {
    local commands; commands=()
    _describe -t commands 'uu_seq commands' commands "$@"
}

if [ "$funcstack[1]" = "_uu_seq" ]; then
    _uu_seq "$@"
else
    compdef _uu_seq uu_seq
fi
