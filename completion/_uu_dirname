#compdef uu_dirname

autoload -U is-at-least

_uu_dirname() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" \
'-z[separate output with NUL rather than newline]' \
'--zero[separate output with NUL rather than newline]' \
'-h[Print help]' \
'--help[Print help]' \
'-V[Print version]' \
'--version[Print version]' \
'::dir:_files' \
&& ret=0
}

(( $+functions[_uu_dirname_commands] )) ||
_uu_dirname_commands() {
    local commands; commands=()
    _describe -t commands 'uu_dirname commands' commands "$@"
}

if [ "$funcstack[1]" = "_uu_dirname" ]; then
    _uu_dirname "$@"
else
    compdef _uu_dirname uu_dirname
fi
